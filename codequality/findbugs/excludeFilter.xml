<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Ignore metamodels -->
    <Match>
        <Class name="~.*_"/>
    </Match>
    <!-- Ignore EI_EXPOSE_REP -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
        <Package name="~com\..*"/>
    </Match>
    <!-- Ignore EI_EXPOSE_REP2 -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/>
        <Package name="~com\..*"/>
    </Match>
</FindBugsFilter>